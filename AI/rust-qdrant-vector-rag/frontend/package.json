    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-plugin-svelte": "^3.12.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "svelte-eslint-parser": "^1.3.2",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@skeletonlabs/skeleton": "^3.2.0",
